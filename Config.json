{
  "name": "QuickGPT",
  "icon": "iconify:logos:openai-icon",
  "identifier": "dev.dyptan.popclip.extension.quickqpt",
  "description": "QuickGPT is an extension for PopClip that integrates OpenAI's powerful language models directly into your MacOS experience",
  "entitlements": ["network"],
  "requirements": ["text"],
  "excludedApps": ["com.apple.Terminal"],
  "macosVersion": "14.0",
  "popclipVersion": 4225,
  "module": "chatgpt.js",
  "options": [
    {
      "identifier": "baseUrl",
      "label": "Base URL",
      "type": "string",
      "defaultValue": "https://api.openai.com/v1/",
      "description": "OpenAI compatible API provider"
    },
    {
      "identifier": "model",
      "label": "Model",
      "type": "string",
      "defaultValue": "gpt-4o",
      "description": "Specify the LLM to use"
    },
    {
      "identifier": "apikey",
      "label": "API Key",
      "type": "secret"
    }
  ]
}